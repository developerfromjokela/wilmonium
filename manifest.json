{
  "manifest_version": 2,
  "name": "Wilmonium",
  "version": "1.0.1",
  "author": "Developer From Jokela",
  "description": "Wilmonium, browser extension for improving Wilma experience in your browser",
  "icons": {
    "48": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/loader.js"]
    }
  ],
  "web_accessible_resources": [
    "js/wilmonium.js",
    "css/wilmonium-dark.css",
    "css/wilmonium.css"
  ]
}